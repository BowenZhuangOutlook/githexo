1. olsnodes -n -t -s(good node)
2. rm -rf grid/oracle home
3. srvctl stop vip -i badnode -f
   srvctl remove vip -i badnode -f
4. crsctl delete node -n badnode
5. mv crsdata/crsconfig/xxx.xml
6. /u01/app/grid/12.1.0.2/oui/bin/runInstaller -updateNodeList ORACLE_HOME=/u01/app/grid/12.1.0.2 "CLUSTER_NODES={sjengopdb02}"
   /u01/app/oracle/product/12.1.0/dbhome_2/oui/bin/runInstaller -updateNodeList ORACLE_HOME=/u01/app/oracle/product/12.1.0/dbhome_2 "CLUSTER_NODES={sjengopdb02}"
7. cd /u01/app/grid/12.1.0.2/addnode
   ./addnode.sh "CLUSTER_NEW_NODES={sjengopdb01}" "CLUSTER_NEW_VIRTUAL_HOSTNAMES={sjengopdb01-vip}"
8. cd $ORACLE_HOME/addnode
   ./addnode.sh "CLUSTER_NEW_NODES={sjengopdb01}"
9. dbca



